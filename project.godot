config_version=5

[application]

config/name="Parallel Processing"
run/main_scene="res://scenes/main_scenes/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://images/icon.png"

[autoload]

InputSets="*res://scripts/global/InputSets.gd"
PlayerManager="*res://scripts/global/PlayerManager.gd"
MultiplayerInput="*res://addons/multiplayer_input/multiplayer_input.gd"
