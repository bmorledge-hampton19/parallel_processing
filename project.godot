[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Parallel Processing"

[editor_plugins]

enabled=PackedStringArray("res://addons/multiplayer_input/plugin.cfg")

[gui]

theme/custom_font="res://fonts/FORCED SQUARE.ttf"

